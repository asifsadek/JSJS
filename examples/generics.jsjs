val num_to_sign = /\(x: num): string => {
  if (x >= 0) then "non-negative" else "negative";
};

// annotated generic function decl
val two_args: [T, U](T, U) -> T = /\[T, U](x: T, y: U): T => {
  x;
};

val apply = /\[T, U](fn: (T) -> U, x: T): U => {
  fn(x);
};

val add = /\(a: num): num => a;

